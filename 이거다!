# 출입국 관광 통계 서비스
#   endpoint
#   http://openapi.tour.go.kr/openapi/service/EdrcntTourismList
#collection.api : collection/api (경로)

import collection.api as pdapi

url = pdapi.pd_gen_url(
    'http://openapi.tour.go.kr/openapi/service/EdrcntTourismStatsService/getEdrcntTourismStatsList',
    'HFG5pLNtWiid4i5Bzyc6a%2FSfNOeiz4HBqoZModpqnj7RFNesMa0h2QP9ivG7RQJ0jml0mGhEccK%2BJfY3SJz3GQ%3D%3D',
    YM='{0:04d}{1:02d}'.format(2018, 1), # 201801
    NAT_CD='130',
    ED_CD='E',
    _type='json'
    )
print(url)

